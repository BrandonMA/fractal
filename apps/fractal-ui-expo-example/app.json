{
    "expo": {
        "jsEngine": "hermes",
        "name": "fractal-ui-expo-example",
        "slug": "fractal-ui-expo-example",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/icon.png",
        "splash": {
            "image": "./assets/splash.png",
            "resizeMode": "contain",
            "backgroundColor": "#ffffff"
        },
        "updates": {
            "fallbackToCacheTimeout": 0
        },
        "assetBundlePatterns": ["**/*"],
        "ios": {
            "supportsTablet": true,
            "bundleIdentifier": "com.brandonma.fractaluiexpoexample",
            "googleServicesFile": "./GoogleService-Info.plist"
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./assets/adaptive-icon.png",
                "backgroundColor": "#FFFFFF"
            },
            "package": "com.brandonma.fractaluiexpoexample",
            "googleServicesFile": "./google-services.json"
        },
        "web": {
            "favicon": "./assets/favicon.png"
        },
        "plugins": [
            "@react-native-firebase/app",
            [
                "expo-build-properties",
                {
                    "ios": {
                        "useFrameworks": "static"
                    }
                }
            ],
            "@react-native-google-signin/google-signin",
            [
                "react-native-fbsdk-next",
                {
                    "appID": "843522126861608",
                    "clientToken": "ed9cebee6b642966c49b0ac2bb9564e8",
                    "displayName": "DB-Manager-Test"
                }
            ]
        ]
    }
}
